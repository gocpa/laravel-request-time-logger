{
    "name": "gocpa/laravel-request-time-logger",
    "description": "Package for logging request time",
    "keywords": [
        "gocpa",
        "laravel",
        "laravel-request-time-logger"
    ],
    "homepage": "https://github.com/gocpa/laravel-request-time-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "Tony V",
            "email": "vaninanton@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^v8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.15"
    },
    "autoload": {
        "psr-4": {
            "GoCPA\\LaravelRequestTimeLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GoCPA\\LaravelRequestTimeLogger\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
